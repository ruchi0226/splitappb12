{
  "_version": "1.65.0",
  "sap.app": {
    "id": "app.splitappb12",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.16.3",
      "toolsId": "5826cd09-a89a-4941-b4e2-59c4b73a742c"
    },
    "crossNavigation": {
      "inbounds": {
        "toolsapp-display": {
          "semanticObject": "toolsapp",
          "action": "display",
          "title": "{{flpTitle}}",
          "subTitle": "{{flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.132.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "app.splitappb12.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "app.splitappb12.view",
        "async": true,
        "viewPath": "app.splitappb12.view"
      },
      "routes": [
        {
          "name": "RouteListitemsView",
          "pattern": "",
          "target": [
            "TargetListitemsView","TargetEmptyView"
          ]
        },
        {
          "name": "RouteDetail",
          "pattern": "detail/{ind}",
          "target": [
            "TargetListitemsView","TargetDetail"
          ]
        }
      ],
      "targets": {
        "TargetListitemsView": {
          "id": "ListitemsView",
          "name": "ListitemsView",
          "controlAggregation": "masterPages",
          "level":1
        },
        "TargetEmptyView": {
          "id": "idEmpty",
          "name": "emptyView",
          "controlAggregation": "detailPages",
          "level":1
        },
        "TargetDetail": {
          "id": "idDetail",
          "name": "DetailView",
          "controlAggregation": "detailPages",
          "level":2
        }
      }
    },
    "rootView": {
      "viewName": "app.splitappb12.view.App",
      "type": "XML",
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "appsplitappb12"
  }
}
  